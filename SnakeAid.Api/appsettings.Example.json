{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog",
      "Serilog.Sinks.Console",
      "Serilog.Sinks.SQLite",
      "Serilog.Enrichers.Environment",
      "Serilog.Enrichers.Span"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithEnvironmentName",
      "WithSpan"
    ],
    "Properties": {
      "Application": "SnakeAid.API"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Application} {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      },
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "logs/logs.db",
          "tableName": "Logs",
          "storeTimestampInUtc": true
        }
      }
    ]
  },
  "Jwt": {
    "SecretKey": "mysuperlongsecretkeythatishard2guess123!",
    "RefreshSecretKey": "anotherultrasecretkeythatmustbe32chars!!",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience",
    "AccessTokenExpirationMinutes": 60,
    "RefreshTokenExpirationDays": 30
  },
  "IdentityOptions": {
    "Password": {
      "RequiredLength": 8,
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireUppercase": true,
      "RequireNonAlphanumeric": false
    },
    "Lockout": {
      "MaxFailedAccessAttempts": 5,
      "DefaultLockoutTimeSpan": "00:15:00",
      "AllowedForNewUsers": true
    },
    "SignIn": {
      "RequireConfirmedEmail": false
    }
  },
  "Authentication": {
    "Google": {
      "ClientId": "hẹ hẹ hẹ"
    }
  },
  "Cloudinary": {
    "CloudName": "YOUR_CLOUD_NAME",
    "ApiKey": "YOUR_API_KEY",
    "ApiSecret": "YOUR_API_SECRET",
    "BaseFolder": "snakeaid"
  },
  "ConnectionStrings": {
    "DefaultConnection": "hẹ hẹ hẹ",
    "SupabaseConnection": "hẹ hẹ hẹ"
  },
  "ApplyMigrations": false,
  "AllowedHosts": "*"
}
